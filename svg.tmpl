        #vertices .degree1  circle { stroke-width: 1.0; } 
        #vertices .degree2  circle { stroke-width: 1.4; } 
        #vertices .degree3  circle { stroke-width: 1.6; } 
        #vertices .degree4  circle { stroke-width: 1.8; } 
        #vertices .degree5  circle { stroke-width: 2.0; } 
        #vertices .degree6  circle { stroke-width: 2.2; } 
        #vertices .degree7  circle { stroke-width: 2.4; } 
        #vertices .degree8  circle { stroke-width: 2.6; } 
        #vertices .degree9  circle { stroke-width: 2.8; } 
        #vertices .degree10 circle { stroke-width: 3.0; } 

        #vertices text { 
          text-anchor: middle; 
          font-size: 1em; 
          font-weight: normal; 
          font-family: sans-serif;
          fill: rgba(255,255,255,0.8); 
          text-shadow: rgba(0, 0, 0, 0.5) 2px 2px 2px ;
        }
        #vertices text.date { font-size: .8em; } 

        #vertices text:hover {
          font-size: 1.4em;
          fill: rgba(255,255,255,1.0); 
          text-shadow: rgba(0, 0, 0, 1) 2px 2px 10px ;
        }
        #vertices .red text:hover {
          text-shadow: rgba(0, 255, 255, 1) 2px 2px 10px ;
        }

        #vertices text.sentence { 
            text-anchor: start;
            visibility: hidden;
        } 
        #vertices g:hover text.sentence { 
            visibility: visible;
        } 

        #vertices text.sentence tspan.subject { 
            font-size: 1.1em;
            fill: rgba(255,255,255,1);
        }

        #vertices text.sentence tspan.a1 { 
            fill: rgba(255,255,255,0.9);
        }
        #vertices text.sentence tspan.rel { 
            fill: rgba(255,255,255,0.6);
        }
        #vertices text.sentence tspan.a2 { 
            fill: rgba(255,255,255,1);
        }

        #edges line { 
            fill: none;
            stroke-linejoin: round;
            stroke-width: 3; 
            stroke: rgba(255,255,255,0.4); 
            z-index: -1; 
        }
        
        #edges line:hover { 
            stroke-width: 5; 
            stroke: rgba(255,255,255,0.8); 
        }

        #edges line.btwns1  { stroke: rgba(0,255,255,0.1); }
        #edges line.btwns2  { stroke: rgba(0,255,255,0.1); }
        #edges line.btwns3  { stroke: rgba(0,255,255,0.2); }
        #edges line.btwns4  { stroke: rgba(0,255,255,0.2); }
        #edges line.btwns5  { stroke: rgba(0,255,255,0.3); }
        #edges line.btwns6  { stroke: rgba(0,255,255,0.4); }
        #edges line.btwns7  { stroke: rgba(0,255,255,0.5); }
        #edges line.btwns8  { stroke: rgba(0,255,255,0.6); }
        #edges line.btwns9  { stroke: rgba(0,255,255,0.7); }
        #edges line.btwns10 { stroke: rgba(0,255,255,0.8); }

    </style>
</head>
<body>
<svg width="{{ width }}" height="{{ height }}" version="1.1" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink">
<filter id="dropshadow" height="130%">
  <feGaussianBlur in="SourceAlpha" stdDeviation="5"/>
    <feOffset dx="3" dy="3" result="offsetblur"/> 
    <feMerge> 
      <feMergeNode/>
      <feMergeNode in="SourceGraphic"/>
   </feMerge>
</filter>

<g transform="translate({{ xw }},{{ xh }}) scale(0.9)">
    <g id="edges">
    {% for e in edges %}
        <line x1="{{e.x1}}" y1="{{e.y1}}" 
              x2="{{e.x2}}" y2="{{e.y2}}" class="btwns{{ e.betweenness|int }}"/>
    {% endfor %}
    </g>
    <g id="vertices">
        {% for v in vertices %}
        <g transform="translate({{ v.x }} {{ v.y }})" 
            class="{{ v.class }} degree{{ v.degree }}" id="{{ v.label }}">
            <circle cx="0" cy="0" r="{{ 10 + v.degree|int }}" style="filter:url(#dropshadow)" />
            {% if v.timestamp|int > 0 %} 
            <text x="0" y="{{ -10 + (v.degree|int / 2) }}" class="date">
                {{ v.timestamp|formatts('%x') }}
            </text>
            {% endif %}
            <text x="0" y="{{ 10 + (v.degree|int / 2) }}" class="label">
            {% if v.uri %} 
                <a xlink:href="{{v.uri}}">{{ v.label }}</a>
            {% else %}
                {{ v.label }}
            {% endif %}
            </text>
            {% if v.subject|length > 0 %}
                <g id="subject"> 
                  <text x="-200" y="{{ 35 + (v.degree|int / 2) }}" class="sentence">
                    <tspan class="subject">{{ v.subject }}</tspan>
                  </text>
                </g>
            {% endif %}
            {% if v.sentences %}
                <g id="sentences"> 
                {% for s in v.sentences %}
                <text x="-200" y="{{ 30 + loop.index * 20 }}" class="sentence">
                    <tspan class="a1">{{ s.a1 }}</tspan>
                    <tspan class="rel">{{ s.rel }}</tspan>
                    <tspan class="a2">{{ s.a2 }}</tspan>
                </text>
                {% endfor %}
                </g>
            {% endif %}
        </g>
        {% endfor %}
    </g>
</g>
</svg>
</body>
